{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Idle Island",
  "version": "0.8.6",
  "identifier": "com.idle-island",
  "build": {
    "frontendDist": "dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "vite",
    "beforeBuildCommand": "tsc && vite build"
  },
  "app": {
    "windows": [
      {
        "title": "Idle Island",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IENDMkQ1RTJBRUM4N0Y4MTkKUldRWitJZnNLbDR0ektOZldmaVFtVmZHZzdqd0taZHJDTThhVCthbHh1VW1lRG1vYXA5MUcwakkK"

    }
  }
}